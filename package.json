{
  "name": "fast-https",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha --config ./.mocharc.js",
    "generate-codebooks": "concurrently -n build: npm:codebook-*",
    "codebook-meta": "ts-node --project ./tools/tsconfig.json ./tools/generate_compression_codebooks.ts -- meta",
    "codebook-targets": "ts-node --project ./tools/tsconfig.json ./tools/generate_compression_codebooks.ts -- targets",
    "codebook-exclusions": "ts-node --project ./tools/tsconfig.json ./tools/generate_compression_codebooks.ts -- exclusions",
    "codebook-securecookies": "ts-node --project ./tools/tsconfig.json ./tools/generate_compression_codebooks.ts -- securecookies",
    "codebook-rules": "ts-node --project ./tools/tsconfig.json ./tools/generate_compression_codebooks.ts -- rules",
    "xml-to-json-rules": "ts-node --project ./tools/tsconfig.json ./tools/xml-to-json-rules.ts",
    "generate-binary-engine": "ts-node --project ./tools/tsconfig.json ./tools/generate-binary-engine.ts",
    "bundle": "rollup --config rollup.config.ts",
    "prebundle": "npm run generate-binary-engine",
    "start:firefox": "web-ext run --source-dir='.'",
    "start:chromium": "web-ext run --source-dir='.' --target chromium",
    "ext:lint": "web-ext lint",
    "ext:build": "web-ext build --overwrite-dest",
    "preext:build": "npm run bundle",
    "format-check": "prettier './{tools,test,src}/**/*.ts' --check",
    "format-fix": "prettier './{tools,test,src}/**/*.ts' --write",
    "lint-check": "tslint './{tools,test,src}/**/*.ts'",
    "lint-fix": "tslint './{tools,test,src}/**/*.ts' --fix",
    "yolo": "(npm run lint-fix || true) && (npm run format-fix || true)"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cliqz/adblocker": "^1.14.4",
    "@remusao/badger": "^1.1.2",
    "fast-xml-parser": "^3.16.0",
    "idb-keyval": "^3.2.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "webextension-polyfill-ts": "^0.15.0"
  },
  "devDependencies": {
    "@remusao/smaz": "^1.8.0",
    "@remusao/smaz-generate": "^1.8.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/chai": "^4.2.11",
    "@types/chrome": "0.0.112",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "rollup": "^2.10.2",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^5.3.0",
    "tslib": "^2.0.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "web-ext": "^4.2.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  }
}
